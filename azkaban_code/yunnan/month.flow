---
config:
  user.to.proxy: hadoop
  failure.emails: noreply@foo.com
  job_params: szmeizi=1

nodes:
 - name: start_job
   type: command
   config:
      command: echo '我要开始了月数据处理。。。'

 - name: app_area_visit_count_job
   type: command
   dependsOn:
     - start_job
   config:
     command: bash ./tool/exec.sh scriptPath=month/app_area_visit_count/1.hive ${job_params}

 - name: export_app_area_visit_count_job
   type: command
   dependsOn:
     - app_area_visit_count_job
   config:
     command: bash ./tool/exec.sh scriptPath=month/app_area_visit_count/export.sqoop ${job_params}

