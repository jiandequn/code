<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ppfuns.report.mapper.AreaOnlineOrderCountDayMapper">
    <sql id="selectAreaListSql">
          select a.*,t.area_name from area_online_order_count_day a
          left join t_area_info t on t.type=a.user_type and t.area_no=a.area_code
            ${ew.customSqlSegment}
    </sql>
    <select id="includeAreaList" resultType="com.ppfuns.report.entity.AreaOnlineOrderCountDay">
      <include refid="selectAreaListSql" />
    </select>
    <select id="includeAreaPage" resultType="com.ppfuns.report.entity.AreaOnlineOrderCountDay">
        <include refid="selectAreaListSql" />
    </select>
</mapper>
