sqoop export \
--connect '%(db_url)s' \
--username %(db_username)s \
--password-file  %(db_pwdfile)s \
--export-dir /%(hive_province)s/hive/reports/app/user_source/day \
--table app_visit_user_source_day \
--columns t_date,parent_column_id,column_code,user_count,visit_count \
--update-key t_date,parent_column_id,column_code \
--update-mode allowinsert \
--input-null-string '\\N'  \
--input-null-non-string '\\N' \
--input-fields-terminated-by '\t' \
--outdir %(sqoop_outdir)s/mr_java
