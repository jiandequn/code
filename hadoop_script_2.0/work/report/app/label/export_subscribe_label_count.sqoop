sqoop export \
--connect '%(db_url)s' \
--username %(db_username)s \
--password-file  %(db_pwdfile)s \
--export-dir /%(hive_province)s/hive/reports/app/label/subscribe_count/total \
--table app_subscribe_label_count \
--columns t_date,parent_column_id,label_id,label_name,subscribe_count \
--update-key t_date,parent_column_id,label_id \
--update-mode allowinsert \
--input-null-string '\\N'  \
--input-null-non-string '\\N' \
--input-fields-terminated-by '\t' \
--outdir %(sqoop_outdir)s/mr_java