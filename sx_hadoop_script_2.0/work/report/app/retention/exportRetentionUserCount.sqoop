sqoop export \
--connect '%(db_url)s' \
--username %(db_username)s \
--password-file  %(db_pwdfile)s \
--export-dir /%(hive_province)s/hive/reports/app/retention/user/day \
--table app_retention_user_count_day \
--columns t_date,parent_column_id,user_type,%(count_field)s \
--update-key t_date,parent_column_id,user_type \
--update-mode allowinsert \
--input-null-string '\\N'  \
--input-null-non-string '\\N' \
-input-fields-terminated-by '\t' \
--outdir %(sqoop_outdir)s/mr_java