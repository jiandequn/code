sqoop import \
--connect '%(db_url)s' \
--username %(db_username)s \
--password-file  %(db_pwdfile)s \
--table t_album_label_manager \
--columns label_id,label_name \
--driver com.mysql.jdbc.Driver \
--fields-terminated-by '\t' \
--delete-target-dir \
--target-dir '/%(hive_province)s/sqoop/album_tmp' \
--num-mappers 1 \
--hive-import \
--hive-database %(hivedb)s \
--hive-table album_label_manager_tmp \
--hive-overwrite \
--outdir %(sqoop_outdir)s/mr_java